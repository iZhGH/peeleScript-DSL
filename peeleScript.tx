Program:
  'begin'
    commands*=Command
  'end'
;

Command:
  InitialCommand | MoveCommand | SpinCommand
;

InitialCommand:
  'initial' x=INT ',' y=INT
;

MoveCommand:
  direction=Direction (steps=INT)?
;

SpinCommand:
  'spin'
;

Direction:
  "up"|"down"|"left"|"right"|"upleft"|"upright"|"downleft"|"downright"
;

Comment:
  /\/\/.*$/
;